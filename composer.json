{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "behat/mink": "^1.10",
        "cloudflare/sdk": "^1.3",
        "derekcodes/turnstile-laravel": "^1.0",
        "dmore/chrome-mink-driver": "^2.8",
        "doctrine/dbal": "^2.0",
        "dusterio/link-preview": "^1.2",
        "econea/nusoap": "^0.9.13",
        "edamov/pushok": "^0.14.1",
        "edujugon/laradoo": "^3.0",
        "enqueue/rdkafka": "^0.10.20",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/promises": "^1.0",
        "illuminate/mail": "7.28.*",
        "illuminate/redis": "7.28.*",
        "irazasyed/telegram-bot-sdk": "^3.4",
        "jenssegers/agent": "^2.6",
        "jenssegers/mongodb": "4.0.0-alpha.1",
        "jrbarros/ssl-check-php": "^1.0",
        "laravel/cashier": "12.17.1",
        "laravel/lumen-framework": "^7.0",
        "laravel/socialite": "^5.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "lkdevelopment/hetzner-cloud-php-sdk": "^2.5",
        "maatwebsite/excel": "^3.1.55",
        "mailchimp/transactional": "^1.0",
        "mpdf/mpdf": "^8.1",
        "nickdnk/zerobounce-php": "^1.3",
        "nordsoftware/lumen-cors": "^3.3",
        "php-amqplib/php-amqplib": "^3.7",
        "propaganistas/laravel-phone": "^4.2",
        "pusher/pusher-php-server": "^7.0",
        "robert-grubb/tiktok-php": "^1.9",
        "smtp2go-oss/smtp2go-php": "^1.1",
        "socialiteproviders/microsoft-azure": "^3.1.0",
        "socialiteproviders/zoho": "^3.0",
        "sonata-project/google-authenticator": "^2.3",
        "spacemudd/laravel-tap-payments": "^2.0",
        "spatie/laravel-permission": "^3.17",
        "th3khan/base64-to-uploaded-file": "dev-main",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },"files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "mouf/nodejs-installer": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
